   <% provide(:title, 'Pianos') %>
    <div class="center jumbotron">
      <h1>Music Market Piano Section</h1>

      <h2>
        This is our catalog of Pianos we have for Sale.
      </h2>
        		<div id="articles" class="clearfix">
        <article>
                    <%= link_to image_tag("p6.jpg", alt: "Pianos")%>
              <h3>Piano 1</h3>
            <ul>
            <li>Brand: <%= Piano.find(1).brand %></li>
            <li>Price: €<%= Piano.find(1).price %></li>
                <li class="dropdown">
                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                  Extra Info <b class="caret"></b>
                </a>
              <ul class="dropdown-menu">
                  <li>Keys: <%= Piano.find(1).keys %></li>
                  <li>Type: <%= Piano.find(1).pianotype %></li>
                  <li>Colour:  <%= Piano.find(1).colour %></li>
                </ul>
              </li>
                </ul>
            <p></p>
                        <% if logged_in? %>
                    <%= link_to "Buy Now", Piano.find(1), class: "btn btn-lg btn-primary" %>
                        <% else %>
                                <%= link_to "Buy Now", login_path, class: "btn btn-lg btn-primary" %>
            <% end %>

            </article>
